# Database Configuration
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/resume_flow
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=resume_flow
POSTGRES_PORT=5432

# Keycloak Authentication
KEYCLOAK_URL=http://localhost:8080
KEYCLOAK_REALM=resume-flow
KEYCLOAK_CLIENT_ID=resume-flow-api
KEYCLOAK_CLIENT_SECRET=your-client-secret-here

# Google Gemini AI
GEMINI_API_KEY=your-gemini-api-key-here

# LiveKit (for AI interviews)
LIVEKIT_URL=wss://your-livekit-server.com
LIVEKIT_API_KEY=your-livekit-api-key
LIVEKIT_API_SECRET=your-livekit-api-secret

# Dynamic DNS (optional)
DYNU_PASS=your-dynu-password

# Application Settings
API_VERSION=5.0.0
ENVIRONMENT=development
API_PORT=8000
LOG_LEVEL=INFO

# Legacy JWT (optional - for backward compatibility)
SECRET_KEY=your-secret-key-for-legacy-jwt
ALGORITHM=HS256
